{
    "name": "@form-validation/validator-identical",
    "version": "2.4.0",
    "description": "The best validation library for JavaScript",
    "license": "https://formvalidation.io/license",
    "homepage": "https://formvalidation.io",
    "author": {
        "name": "Nguyen Huu Phuoc",
        "email": "me@phuoc.ng",
        "url": "https://twitter.com/nghuuphuoc"
    },
    "keywords": [
        "validate",
        "validator"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/amd/*.*",
        "lib/cjs/*.*",
        "lib/esm/*.*",
        "lib/umd/*.*",
        "lib/index.d.ts",
        "lib/index.js"
    ],
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@form-validation/core": "2.4.0"
    },
    "scripts": {
        "clean": "rm -rf lib && mkdir lib",
        "build:js": "cp src/index.d.ts lib && cp dist/index.js lib && rollup -c ../../rollup.config.cjs",
        "release": "npm publish --access public"
    }
}
